@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfc: <https://w3id.org/rdf-connect#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.

### WriteReplication ###
rdfc:WriteReplication rdfc:jsImplementationOf rdfc:Processor;
    rdfc:file <./lib/index.js>;
    rdfc:class "WriteReplication";
    rdfc:entrypoint <./>.

[ ] a sh:NodeShape;
    sh:targetClass rdfc:WriteReplication;
    sh:property [
        sh:class rdfc:Reader;
        sh:path rdfc:incoming;
        sh:name "incoming";
        sh:maxCount 1;
        sh:minCount 1;
    ], [
        sh:datatype xsd:boolean;
        sh:path rdfc:append;
        sh:name "append";
        sh:minCount 0;
        sh:maxCount 1;
    ], [
        sh:datatype xsd:string;
        sh:path rdfc:savePath;
        sh:name "savePath";
        sh:minCount 1;
        sh:maxCount 1;
    ], [
        sh:datatype xsd:integer;
        sh:path rdfc:max;
        sh:name "max";
        sh:minCount 0;
        sh:maxCount 1;
    ].


### ReadReplication ###
rdfc:ReadReplication rdfc:jsImplementationOf rdfc:Processor;
    rdfc:file <./lib/index.js>;
    rdfc:class "ReadReplication";
    rdfc:entrypoint <./>.

[ ] a sh:NodeShape;
    sh:targetClass rdfc:ReadReplication;
    sh:property [
        sh:class rdfc:Writer;
        sh:path rdfc:outgoing;
        sh:name "outgoing";
        sh:maxCount 1;
        sh:minCount 1;
      ], [
        sh:datatype xsd:string;
        sh:path rdfc:savePath;
        sh:name "savePath";
        sh:minCount 1;
        sh:maxCount 1;
    ].
